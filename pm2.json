{
  "apps": {
    "name": "face_railway",
    "script": "./bin/www",
    "watch": true,
    "ignore_watch": [
      "node_modules",
      "src",
      "logs",
      "build",
      "bin",
      "docker",
      "config",
      "uploads"
    ],
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "max_restarts": 300,
    "autorestart": true,
    "cron_restart": "",
    "restart_delay": 60
  }
}